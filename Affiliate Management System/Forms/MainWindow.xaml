        <Button x:Name="btnSearchF" TextBlock.FontSize="20" TextBlock.FontFamily="Times New Roman" FontWeight="Bold" HorizontalAlignment="Left" Height="47" Margin="60,429,0,0" VerticalAlignment="Top" Width="126" Click="btnSave_Click">
